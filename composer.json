{
	"name": "vinala/app",
	"description": "Vinala, An elegant and swift PHP Framework For Webmasters",
	"keywords": [
		"Framework",
		"Vinala"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Youssef Had",
			"email": "youssefhad2@gmail.com"
		}
	],
	"require": {
		"php": ">=5.5.9"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"smarty/smarty": "~3.1.21",
		"swiftmailer/swiftmailer": "5.3.1",
		"monolog/monolog": "~1.17.2",
		"nesbot/carbon": "~1.14",
		"vinala/kernel": "dev-master",
		"vinala/whoops": "1.2.*",
		"symfony/console": "3.*",
		"youssef/debug": "1.*",
		"vlucas/valitron": "^1.2",
		"phpunit/phpunit": "~4.1",
		"symfony/cache": "3.*",
		"symfony/process": "3.2.*",
		"tedivm/stash": "^0.14.1"
	},
	"scripts": {
		"post-create-project-cmd": [
			"php lumos setup"
		]
	},
	"autoload": {
		"files": [
			"vendor/vinala/kernel/src/Support/Helpers.php",
			"vendor/vinala/kernel/src/Support/Helper.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.3.*-dev",
			"dev-dev": "3.3.*-dev"
		}
	}
}